<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maaz.admincollegeapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminCollegeApp"
        >
        <activity android:name=".Notice.DeleteNoticeActivity"
            android:label="Delete Notices"
            />
        <activity android:name=".Faculty.UpdateTeacherActivity"
            android:label="Update Teacher"
            />
        <activity android:name=".Faculty.AddTeature"
            android:label="Add Techer"
            />
        <activity android:name=".Faculty.UpdateFaculty"
            android:label="Update Faculty"
            />
        <activity android:name=".UploadPdf"
            android:label="Upload Pdf"
            />
        <activity android:name=".UploadImage"
            android:label="Upload Image"
            />
        <activity android:name=".Notice.UploadNotice"
            android:label="Upload Notice"
            />
        <activity android:name=".MainActivity"
            android:label="Admin Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts"
            />
    </application>

</manifest>